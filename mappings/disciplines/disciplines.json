{
  "request": {
    "method": "GET",
    "urlPath": "/api/disciplines",
    "queryParameters" : {
      "size": {
        "or": [
          {
            "matches": "[0-9]+"
          },
          {
            "absent": true
          }
        ]
      },
      "page": {
        "or": [
          {
            "matches": "[0-9]+"
          },
          {
            "absent": true
          }
        ]
      }
    }
  },
  "response": {
    "status": 200,
    "bodyFileName": "disciplines/disciplines.json",
    "headers": {
      "Content-Type": "application/json",
      "Access-Control-Allow-Origin": "*",
      "Access-Control-Allow-Methods": "*"
    }
  }
}
